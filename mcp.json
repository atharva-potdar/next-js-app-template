{
  "mcpServers": {
    "nextjs-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "next-devtools-mcp@latest"
      ]
    },
    "prisma-mcp": {
      "url": "https://mcp.prisma.io/mcp"
    },
    "shadcn-mcp": {
      "command": "npx",
      "args": [
        "shadcn@latest",
        "mcp"
      ]
    }
  }
}
